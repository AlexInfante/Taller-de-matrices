#include <stdio.h>
int main (){
int matriz [3][3];//declarar matriz 3x3
    matriz [0][0]=9;
    matriz [0][1]=8;
    matriz [0][2]=7;
    matriz [1][0]=6;
    matriz [1][1]=5;
    matriz [1][2]=4;
    matriz [2][0]=3;
    matriz [2][1]=2;
    matriz [2][2]=1;
int matrizSuma[3][3];
    for (int i=0; i<3; i++){
        for (int j=0; j<3; j++){
            matrizSuma [i][j]= matriz[i][j]+matriz[i][j];
    
    printf ("%d\t", matrizSuma[i][j]);
    }printf ("\n");
    }
return 0;
}
